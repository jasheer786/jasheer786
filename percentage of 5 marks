#include<stdio.h>
int main()
{
int phy,che,bio,mat,cs;
float percentage;
printf("Enter five subjects mark:");
scanf("%d %d %d %d %d ",&phy,&che,&bio,&mat,&cs);
percentage=(phy+che+bio+mat+cs)/5.0;
printf("percentage=%2f\n",percentage);
if(percentage>=90)
{
printf("Grade A");
}
else if(percentage>=80)
{
printf("Grade B");
}
else if(percentage>=70)
{
printf("Grade C");
}
else if(percentage>=60)
{
printf("Grade D");
}
else if(percentage>=50)
{
printf("Grade E");
}
else
{
printf("Fail");
}
return 0;
}
